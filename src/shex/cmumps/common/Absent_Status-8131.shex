PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX Map: <http://shex.io/exts/Map/#>
PREFIX absentStatus: <urn:local:absentStatus:>
PREFIX cmumpss: <http://hokukahu.com/schema/cmumpss#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
BASE <http://hl7.org/fhir/shape/>

start=@<http://hokukahu.com/Absent_Status-8131>

<http://hokukahu.com/Absent_Status-8131> {
    cmumpss:identifier                                xsd:string        %Map:{ absentStatus:id %};
    cmumpss:cmumps_type                               xsd:string?       %Map:{ absentStatus:cmumps_type %};
    rdfs:label                                        xsd:string        %Map:{ absentStatus:label %};
    cmumpss:name-8131                                 xsd:string        %Map:{ absentStatus:name %};
    cmumpss:code-8131                                 xsd:string        %Map:{ absentStatus:code %};

    cmumpss:army_chargeable-8131                      xsd:boolean?      %Map:{ absentStatus:army_chargeable %};
    cmumpss:air_force_chargeable-8131                 xsd:boolean?      %Map:{ absentStatus:air_force_chargeable %};
    cmumpss:marines_chargeable-8131                   xsd:boolean?      %Map:{ absentStatus:marines_chargeable %};
    cmumpss:navy_chargeable-8131                      xsd:boolean?      %Map:{ absentStatus:navy_chargeable %};
    cmumpss:insurance_chargeable-8131                 xsd:boolean?      %Map:{ absentStatus:insurance_chargeable %};

    cmumpss:activation_inactivation_date-8131 {
       cmumpss:identifier                             xsd:string        %Map:{ absentStatus:activation_inactivation_date_id %};
       cmumpss:cmumps_type                            xsd:string?       %Map:{ absentStatus:activation_inactivation_date_type %};
       cmumpss:activation_inactivation_date-8131_01   xsd:date or
                                                      xsd:dateTime or
                                                      xsd:datetime      %Map:{ absentStatus:activation_inactivation_date %};
       cmumpss:active_inactive_status-8131_01         xsd:string        %Map:{ absentStatus:active_inactive_status %};
    }*;
}
